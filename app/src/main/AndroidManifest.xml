<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chenxh.myttestapplication">

    <uses-permission android:name="android.permission.SOCSI_SDK_SYS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <!-- home键权限获取 -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TwoActivity" />
        <activity android:name=".activity.SecondActivity" />
        <activity
            android:name=".activity.PayActivity"
            android:label="@string/title_activity_pay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".diyView.demo1.DiyViewActivity" />
        <activity android:name=".activity.SPActivity" />

        <service android:name=".service.ExampleService" />

        <activity android:name=".activity.ServiceActivity" />
        <activity android:name=".activity.PopDiaLogActivity" />
        <activity android:name=".activity.PrintActivity" />
        <activity android:name=".activity.Test12Activity" />
        <activity android:name=".activity.AActivity">
            <intent-filter>
                <action android:name="action_a" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BActivity">
            <intent-filter>
                <action android:name="action_b" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PermiisionActivity" />
        <activity android:name=".activity.GradLayoutActivity" />
        <activity android:name=".activity.AnimationActivity" />
        <activity android:name=".activity.WifiActivity" />
        <activity android:name=".activity.BitmapIOActivity"></activity>
    </application>

</manifest>